{
  "expo": {
    "name": "chat-client",
    "slug": "chat-client",
    "version": "1.0.0",
    "scheme": "fortune",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.chatclient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.chatclient"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro", 
      "output": "single"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2da70a02-c27a-4ec7-85b2-260e09d688a9"
      }
    }
  }
}
